<script type="text/javascript">
    function displayBioPanel() {
        return hl_directory_profile_hideShowEditMode('#MainCopy_ctl27_pnlSummaryEdit', '#MainCopy_ctl27_pnlSummaryDisplay');
    }
    function hideBioPanel() {
        return hl_directory_profile_hideEditMode('MainCopy_ctl27_pnlSummaryDisplay');
