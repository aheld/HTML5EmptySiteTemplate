									function GlobalApplicationLoadHandler(sender, args) {
										var prm = Sys.WebForms.PageRequestManager.getInstance();
										if (!prm.get_isInAsyncPostBack()) {
											prm.add_initializeRequest(initRequest);
											prm.add_endRequest(endRequest);
										}
									}
