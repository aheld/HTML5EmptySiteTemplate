        function toggleHelp() {
            
            if ($("ul#CompleteBarHelpContainer").is(":visible")) {
                $("ul#CompleteBarHelpContainer").hide();
            } else {
                $("ul#CompleteBarHelpContainer").show();
            }
        }

        $("span.CompleteBarClose").live('click', function (e) {

            e.stopPropagation();
            toggleHelp();
